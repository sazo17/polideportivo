                <ImageBrush ImageSource="polideportivo_images/Equipos/ejemplo1.png"/>
            </Rectangle.Fill>
        </Rectangle>
        <Label x:Name="lbl_nombre_equipo_Copy" Content="FC Barcelona Vs. FC Bacelona1" HorizontalAlignment="Left" Margin="196,284,0,0" VerticalAlignment="Top" Foreground="#FFFDEFEF" Height="39" Cursor="Hand" Width="386" HorizontalContentAlignment="Center" RenderTransformOrigin="-1.886,-6.115" FontWeight="Bold" FontSize="18"/>
        <Label x:Name="lbl_nombre_equipo_Copy1" Content="FC Barcelona Vs. FC Bacelona1" HorizontalAlignment="Left" Margin="199,404,0,0" VerticalAlignment="Top" Foreground="#FFFDEFEF" Height="39" Cursor="Hand" Width="386" HorizontalContentAlignment="Center" RenderTransformOrigin="-1.886,-6.115" FontWeight="Bold" FontSize="18"/>
        <Label x:Name="lbl_ayuda_Copy" Content="Finalizado" HorizontalAlignment="Left" Margin="277,189,0,0" VerticalAlignment="Top" Foreground="#FFFDEFEF" Height="26" Cursor="Hand" Width="221" HorizontalContentAlignment="Center" RenderTransformOrigin="0.47,1.385"/>
        <Label x:Name="lbl_ayuda_Copy1" Content="25 de Marzo, 2018" HorizontalAlignment="Left" Margin="277,318,0,0" VerticalAlignment="Top" Foreground="#FFFDEFEF" Height="26" Cursor="Hand" Width="221" HorizontalContentAlignment="Center" RenderTransformOrigin="0.47,1.385"/>
        <Label x:Name="lbl_ayuda_Copy2" Content="26 de Marzo, 2018" HorizontalAlignment="Left" Margin="277,434,0,0" VerticalAlignment="Top" Foreground="#FFFDEFEF" Height="26" Cursor="Hand" Width="221" HorizontalContentAlignment="Center" RenderTransformOrigin="0.47,1.385"/>
        <Button x:Name="btn_agregar_estadisticas" Content="Agregar Estadísticas" HorizontalAlignment="Left" Margin="327,131,0,0" VerticalAlignment="Top" Width="131" RenderTransformOrigin="0.893,-2.95" Click="btn_agregar_estadisticas_Click"/>

    </Grid>
</Window>
